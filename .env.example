# Telegram bot token from BotFather
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE

# Path to the Google service account JSON file (absolute or relative)
GOOGLE_SERVICE_ACCOUNT_JSON=./service_account.json

# ID of the Google Sheets document (from the URL)
SPREADSHEET_ID=YOUR_SPREADSHEET_ID_HERE

# Worksheet names (can be changed if needed)
INCOME_SHEET_NAME=Income
EXPENSES_SHEET_NAME=Expenses

# Optional settings
LOG_LEVEL=INFO
